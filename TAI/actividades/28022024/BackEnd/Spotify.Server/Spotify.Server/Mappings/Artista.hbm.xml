<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
				   schema="dbo" 
				   assembly="Spotify.Server" 
				   namespace="Spotify.Server.Mappings" 
				   default-lazy="false"
				   default-cascade="none">
	<class name="Spotify.Server.Entities.Artista" table="Artista">
		<id name="Id" column="Id" type="Int32" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="NombreArtistico" column="NombreArtistico" type="AnsiString"/>
		<property name="Nacionalidad" column="Nacionalidad" type="AnsiString"/>
		<property name="GeneroMusical" column="GeneroMusical" type="AnsiString"/>
		<property name="FechaNacimiento" column="FechaNacimiento" type="DateTime2"/>
		<property name="Genero" column="Genero"/>
	</class>
</hibernate-mapping>
